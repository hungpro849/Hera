<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="80dp"
    tools:context=".User.lineSachDangMuon">

    <ImageView
        android:id="@+id/MU_img"
        android:layout_width="79dp"
        android:layout_height="55dp"
        android:layout_marginBottom="10dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/bookex" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="120dp"
        android:layout_height="20dp"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="Số lần gia hạn:"
        app:layout_constraintStart_toEndOf="@+id/MU_img"
        app:layout_constraintTop_toBottomOf="@+id/MU_line_name" />

    <TextView
        android:id="@+id/MU_line_name"
        android:layout_width="216dp"
        android:layout_height="20dp"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="15dp"
        android:text="Phân tích và thiết kế giải thuật"
        app:layout_constraintStart_toEndOf="@+id/MU_img"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/SDM_tV_langiahan"
        android:layout_width="50dp"
        android:layout_height="20dp"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="1"
        app:layout_constraintStart_toEndOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/MU_line_name" />
</android.support.constraint.ConstraintLayout>